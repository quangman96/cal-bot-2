{
  "name": "Calculator Bot zxc",
  "description": "Simple calculator bot !!!",
  "version": "1.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "*://futures.mexc.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "eps.js",
        "caculator.js"
      ],
      "css": [
        "eps.css"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "clipboardRead"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "Extension",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/mm16.png",
      "32": "/images/mm32.png",
      "48": "/images/mm48.png",
      "128": "/images/mm128.png"
    }
  },
  "icons": {
    "16": "/images/mm16.png",
    "32": "/images/mm32.png",
    "48": "/images/mm48.png",
    "128": "/images/mm128.png"
  }
}