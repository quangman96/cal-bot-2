{
  "name": "Calculator Bot",
  "description": "Simple calculator bot !!!",
  "version": "2.2",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "*://futures.mexc.com/*"
      ],
      "run_at": "document_end",
      "js": [
        "eps.js"
      ],
      "css": [
        "eps.css"
      ]
    }
  ],
  "permissions": [],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "Extension",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/hgnc16.png",
      "32": "/images/hgnc32.png",
      "48": "/images/hgnc48.png",
      "128": "/images/hgnc128.png"
    }
  },
  "icons": {
    "16": "/images/hgnc16.png",
    "32": "/images/hgnc32.png",
    "48": "/images/hgnc48.png",
    "128": "/images/hgnc128.png"
  }
}